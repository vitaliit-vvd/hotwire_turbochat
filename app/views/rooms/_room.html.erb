<%= turbo_frame_tag dom_id(room), class: "",
                    data: { controller_name: "#{controller_name}",
                            action_name: "#{action_name}" } do %>
	<%= link_to room_name(room), room_path(room.title),
	            class: "text-current text-blue-500 mr-4 bg-gray-800 py-3 px-4 rounded-md mt-4",
	            data: { turbo_frame: :rooms }  %>
<% end %>
