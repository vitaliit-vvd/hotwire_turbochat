<% if @user.errors.any? %>
	<%= turbo_stream.replace "profile_form", partial: 'devise/registrations/form', locals: { resource: @user } %>
<% else %>
	<%= render_flash %>
	<%= turbo_stream.update :rooms, target: "user_#{@user.id}", partial: 'users/user', locals: { profile_user: @user, user: current_user } %>
<% end %>
